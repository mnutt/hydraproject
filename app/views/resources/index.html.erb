<h1>Listing resources</h1>

<table>
  <tr>
    <th>File file name</th>
    <th>File content type</th>
    <th>File file size</th>
    <th>File updated at</th>
  </tr>

<% for resource in @resources %>
  <tr>
    <td><%=h resource.file_file_name %></td>
    <td><%=h resource.file_content_type %></td>
    <td><%=h resource.file_file_size %></td>
    <td><%=h resource.file_updated_at %></td>
    <td><%= link_to 'Show', resource %></td>
    <td><%= link_to 'Edit', edit_resource_path(resource) %></td>
    <td><%= link_to 'Destroy', resource, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New resource', new_resource_path %>
